          <%} %>
        </li>
        <li class="nav-item">
          <% if (title == "Project") { %>
          <a class="nav-link active" aria-current="page" href="/project"><i class="fa-solid fa-lg fa-briefcase"></i> Project</a>
          <% } else {%>
            <a class="nav-link" aria-current="page" href="/project"><i class="fa-solid fa-lg fa-briefcase"></i> Project</a>
            <%} %>
        </li>
        <li class="nav-item">
          <% if (title == "Contact us") { %>
          <a class="nav-link active" aria-current="page" href="/contactus"><i class="fa-solid fa-lg fa-phone-volume"></i> Contact us</a>
          <% } else {%>
            <a class="nav-link" aria-current="page" href="/contactus"><i class="fa-solid fa-lg fa-phone-volume"></i> Contact us</a>
            <%} %>
        </li>
    </div>
  </div>
</nav>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %> page</p>
    <script src="/script/app.js"></script>
